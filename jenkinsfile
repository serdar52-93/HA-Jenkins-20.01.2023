pipeline {
  agent any
  tools {
    maven 'maven-3.8.7' 
  }
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
    }

    stage ('Test') {
        steps {
         echo "Test" 
         sh 'mvn test'
        }
    }

    stage('Deploy') { 
        steps {   
         echo "Deploy" 
        }
    } 
  }
}